{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "quotes": ["warning", "single"],
        "semi": ["warning", "always"],
        "eol-last": ["error", "windows"]
    }
}
